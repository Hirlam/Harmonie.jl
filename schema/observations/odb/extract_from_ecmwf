


# $var is one of 
# reporttype
# subtype
# obstype
# satobkind
# etc. 


var=$1


for var in reporttype subtype obstype satobkind varno
do 
  url="https://apps.ecmwf.int/odbgov/$var.json" 
  jsondata=$(wget -qO- $url) 
  echo $jsondata | jq -f ecfilter.jq --arg url "${url}"  --arg var ${var}
done 





